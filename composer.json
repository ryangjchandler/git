{
    "name": "ryangjchandler/git",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "homepage": "https://ryangjchandler.co.uk",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "RyanChandler\\Git\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RyanChandler\\Git\\Tests\\": "tests/"
        }
    },
    "require": {
        "titasgailius/terminal": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
